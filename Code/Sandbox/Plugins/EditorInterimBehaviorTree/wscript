def build(bld):	
	bld.CryEditorPlugin(
		target = 'EditorInterimBehaviorTree',
		vs_filter = 'Sandbox/Plugins',
		file_list = 'editorinterimbehaviortree.waf_files',
		pch = 'StdAfx.cpp',
		features = 'qt',
		includes = ['.', '..'],
		use=['Editor', 'EditorCommon' ]
	)
